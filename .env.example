# Discord Shit Tracker Bot Configuration
# Copy this file to .env and fill in your actual values

# Discord Bot Token (required)
# Get from https://discord.com/developers/applications
DISCORD_BOT_TOKEN=your_bot_token_here

# OpenRouter API Key (required for text improvement)
# Get from https://openrouter.ai/
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Target Configuration (required)
# Get these IDs by right-clicking in Discord with Developer Mode enabled
TARGET_GUILD_ID=your_discord_server_id_here

# Optional Configuration
LOG_LEVEL=INFO
COMMAND_PREFIX=!st 